function listenToClicks()
{
	var domains=["apa.org", "apapracticecentral.org", "apacommunities.org", "apadivisions.org", "apaedat.org", "apapracticenet.net", "apastyle.org", "decadeofbehavior.org", "jbo.com", "kidspsych.org", "phwa.org", "teachpsych.com", "teachpsych.org", "apadiv2.org", "yourmindyourbody.org", "supportpsychologypac.org"];
	var fileTypes=[".doc",".xls",".exe",".zip",".pdf",".mov",".mp3"];
	jQuery('a').each(function(index) {
		var link = jQuery(this);
		jQuery.each(domains, function(j) {
			try
			{
				if((jQuery(link).attr('href').indexOf(domains[j])!=-1)&&(window.location.href.indexOf(domains[j])==-1)){	
					var valid = true;
					
					jQuery.each(fileTypes, function(i) {
						if(jQuery(link).attr('href').indexOf(fileTypes[i])!=-1){
							valid = false;
						}
					});
					
					if (valid)
					{
						jQuery(link).bind('click', function(l) {
							if(typeof(_gat)=="object"){
								l.preventDefault();
								if (jQuery(link).attr('target') != "_blank")
								{
									_gaq.push(['_link',jQuery(link).attr('href')]);
								}
						 		else
						 		{
				 					var tracker = _gat._getTrackerByName();
									var fullUrl = tracker._getLinkerUrl(jQuery(link).attr('href'));
									window.open(fullUrl);
						 		}
							}
						});
					}
				}
			}
			catch(e)
			{
				//Bad A tag
			}
			
		});
	});
}

jQuery(document).ready(function() {
	listenToClicks();
});O±bplist00ÔpqT$topX$objectsX$versionY$archiverÑ_WebResourceResponse¯"	
